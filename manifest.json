{
  "name": "Mileage Ticket",
  "version": "1.0",
  "description": "Subscribe to the latest mileage ticket releases!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*.ana.co.jp/*",
    "*://www.baidu.com/*",
    "*://www.google.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Let's see what it can do",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "manifest_version": 2
}
